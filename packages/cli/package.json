{
  "name": "@frc-web-components/cli",
  "version": "4.3.0-alpha.1",
  "description": "",
  "bin": {
    "frc-web-components": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/cli.ts --minify && tsup src/dashboard.ts --minify --format iife --legacy-output && tsup src/add-plugins.ts --format esm --legacy-output",
    "dev": "tsup src/cli.ts --watch",
    "test": "",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "generate-docs": "jsdoc -c jsdoc.conf.json",
    "lint": "eslint src/**/*",
    "lint:format": "eslint src/**/* --fix",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "preversion": "npm run lint",
    "version": "npm run lint && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frc-web-components/components.git"
  },
  "author": "Amory Galili",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frc-web-components/components/issues"
  },
  "homepage": "https://github.com/frc-web-components/components#readme",
  "dependencies": {
    "@frc-web-components/dashboard": "*",
    "@frc-web-components/frc-web-components": "*",
    "@frc-web-components/plugins": "*",
    "@webbitjs/store": "^5.5.0",
    "@webbitjs/webbit": "^5.5.0",
    "chalk": "^5.0.1",
    "commander": "^9.4.0",
    "vite": "^2.3.8",
    "vite-plugin-html": "^3.2.0"
  },
  "devDependencies": {
    "tsup": "^5.12.1"
  }
}
