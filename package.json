{
  "name": "frc-web-components",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm --filter @frc-web-components/fwc dev",
    "build": "nx run-many -t build",
    "watch:fwc": "pnpm --filter @frc-web-components/fwc watch",
    "watch:lit": "pnpm --filter @frc-web-components/lit watch",
    "watch:react": "pnpm --filter @frc-web-components/react watch",
    "watch:svelte": "pnpm --filter @frc-web-components/svelte watch",
    "watch:app": "pnpm --filter @frc-web-components/app watch",
    "watch:example-app": "pnpm --filter example-app dev",
    "watch": "concurrently \"npm:watch:*\"",
    "build:fwc": "nx build @frc-web-components/fwc",
    "build:lit": "nx build @frc-web-components/lit",
    "build:react": "nx build @frc-web-components/react",
    "build:svelte": "nx build @frc-web-components/svelte",
    "storybook": "pnpm --filter storybook storybook",
    "prepare": "husky install",
    "test": "",
    "preinstall": "npx only-allow pnpm"
  },
  "private": true,
  "devDependencies": {
    "@nx/js": "17.1.3",
    "concurrently": "^8.2.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.3",
    "nx": "17.1.3",
    "typescript": "^5.3.2"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ]
}
